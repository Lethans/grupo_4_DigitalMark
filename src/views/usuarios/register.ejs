    <title>Registrate</title>
</head>

<body>

    <main class="container-fluid">

        <%- include('../partials/navBar')%>

        <div class="row col-12 col-md-12 col-lg-12" style="margin:0px">



            <div class="form-group col-12" style="text-align:center">


                <div class="container">

                    <div class="d-flex justify-content-center h-100">

                        <span id="loading" class="tests">
                            <div class="LoaderBalls">
                                <div class="LoaderBalls__item"></div>
                                <div class="LoaderBalls__item"></div>
                                <div class="LoaderBalls__item"></div>
                            </div>
                        </span>
                        <!-- <div id="loading" class="loader"></div> -->

                        <form onsubmit="event.preventDefault(); myValidation();" id="registerForm" action="/register"
                            method="post" enctype="multipart/form-data">

                            <div id="formCard" class="card">
                                <div class="card-header">
                                    <h3>Registrarse</h3>

                                    <h6 class="modal-title p-login"
                                        style="text-align:center;padding: 0px;color: white;">¿Ya
                                        tenés
                                        cuenta? <a href='/login' style="color: orange;">Ingresá</a></h6>

                                </div>
                                <div class="card-body bodyTop">

                                    <% if(typeof errors != 'undefined'){%>
                                    <script>
                                        var card = document.getElementById('formCard');
                                        card.style.height = "620px";
                                    </script>
                                    <%}%>

                                    <small id="userNameError" style="display: none;"
                                        class="form-text text-danger errorInput">
                                        Campo obligatorio</small>
                                    <div class="input-group form-group">
                                        <div class="input-group-prepend">
                                            <span id="nameIcon" class="input-group-text"><i
                                                    class="fas fa-user"></i></span>
                                        </div>
                                        <input id="userName" type="text" name="firstName" class="form-control"
                                            placeholder="<%=typeof errors != 'undefined' && errors.firstName != null ? errors.firstName : 'Nombre' %>"
                                    value="<%= typeof old != 'undefined' ? old.firstName : '' %>">
                                </div>
                                <% if(typeof errors != 'undefined' && errors.firstName){%>
                                <script>
                                    onErrorName();
                                </script>
                                <%}%>

                                    
                                    <small id="userLastNameError" style="display: none;"
                                        class="form-text text-danger errorInput">
                                        Campo obligatorio</small>
                                    <div class="input-group form-group">
                                        <div class="input-group-prepend">
                                            <span id="lastNameIcon" class="input-group-text"><i class="fas fa-user"></i></span>
                                        </div>
                                        <input id="userLastName" type="text" name="lastName" class="form-control"
                                            placeholder="<%=typeof errors != 'undefined' && errors.lastName != null ? errors.lastName : 'Apellido' %>"
                                value="<%= typeof old != 'undefined' ? old.lastName : '' %>">
                            </div>
                            <% if(typeof errors != 'undefined' && errors.lastName){%>
                            <script>
                                onErrorLastName();
                            </script>
                            <%}%>
                                    

                                    <small id="userEmailError" style="display: none;"
                                        class="form-text text-danger errorInput">
                                        Campo obligatorio</small>
                                    <div class="input-group form-group">
                                        <div class="input-group-prepend">
                                            <span id="emailIcon" class="input-group-text"><i class="fas fa-envelope"></i></span>
                                        </div>
                                        <input id="userEmail" type="email" name="email" class="form-control"
                                            placeholder="<%=typeof errors != 'undefined' && errors.email != undefined ? errors.email.msg : 'Email'%>"
                            value="">
                    </div>

                    <% if(typeof errors != 'undefined' && errors.email){%>
                    <script>
                        onErrorEmail();
                    </script>
                    <%}%>


                                
                            <small id="userPasswordError" style="display: none;"
                                class="form-text text-danger errorInput">Obligatorio mayuscula,minuscula/1
                                numero</small>
                            <div class="input-group form-group">
                                <div class="input-group-prepend">
                                    <span id="first_eye" class="input-group-text"><i
                                            class="fas fa-eye-slash"></i></span>
                                </div>
                                <input id="userPassword" type="password" name="password" value=""
                                    class="form-control first_password" placeholder="<%=typeof errors != 'undefined' && errors.password != undefined ? errors.password.msg : 'Mays, mins, numero, 6 caracteres' %>">
                </div>

                <% if(typeof errors != 'undefined' && errors.password){%>
                <script>
                    onErrorPassword();
                </script>
                <%}%>
    
                <small id="userConfirmPasswordError" style="display: none;"
                    class="form-text text-danger errorInput">
                    Campo obligatorio</small>
                <small id="userConfirmPasswordError2" style="display: none;"
                    class="form-text text-danger errorInput">Las contraseñas no coinciden</small>
                <div class="input-group form-group">
                    <div class="input-group-prepend">
                        <span id="second_eye" class="input-group-text"><i class="fas fa-eye-slash"></i></span>
                    </div>
                    <input id="userConfirmPassword" type="password" name="confirm_password"
                        class="form-control second_password" placeholder="<%=typeof errors != 'undefined' && errors.confirm_password != undefined ? errors.confirm_password.msg : 'Confirme contraseña' %>">
            </div>
            <% if(typeof errors != 'undefined' && errors.confirm_password){%>
            <script>
                onErrorConfirmPassword();
            </script>
            <%}%>
                                

            <small id="avatarError" style="display: none;"
                                        class="form-text text-danger errorInput">
                                        Campo obligatorio</small>
        <div class="custom-file uploadFile">
            <input type="file" name="avatar" id="img" style="display:none;" />
            <label for="img" class="profile_pic" style="cursor:pointer">
                <i class="far fa-images"></i>
                Subi tu foto</label>
        </div>
        <% if(typeof errors != 'undefined' && errors.avatar){%>
            <p style="font-weight: bold;margin-top: 10px;" class="text-danger">
                <%= errors.avatar.msg %></p>
            <%}%>


                            </div>
                            <div class="form-group card-footer">
                                <input type="submit" value="Registrar" class="btn float-right login_btn">
                                <!-- <button onclick="signUp()" type="button" type="submit" class="btn btn-outline-primary text-uppercase mb-3"
                                >Sign Up</button> -->
                            </div>

                        </form>



                </div>
            </div>
        </div>
    </div>
    </main>
    <%- include('../partials/boostrapScripts')%>

</body>

</html>